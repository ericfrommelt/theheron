:@template"L#img { max-width:100%; }
a { text-decoration: none; }

#heron-header {
	
}

#menu, #hours, #our-story, #catering, #suppliers, #contact-us {
	padding-top:50px;
}

$font-sans: "omnes-pro",sans-serif;

/* Navigation
=====================================================================*/
#heron-nav {
	background-color: #f8efe6;
	position: fixed;
	top: 0;
	width: 100%;
	z-index: 9999;
	
}

#heron-nav li {
	float:left;
	font-family: $font-sans;
	font-style: normal;
	font-weight: 400;
	text-transform:uppercase;
	font-size:18px;
	padding:0 20px;
	margin-top:24px;
}

#heron-nav li:nth-child(1) {
	border-left: 1px solid #101415;
	padding-left:20px;
}

#heron-nav a {
	color:#101415;
	text-decoration:none;
}

.logo {
	float:left;
	padding-right:10px;
	margin-top:10px;
}

/* Image Gallery
=====================================================================*/

#img-gallery {
	width: 100%;
	position:fixed;
	background:#2f2b28;
   /* Prevent vertical gaps */
   line-height: 0;
   

   
   
   
}
#img-gallery img {
  /* Just in case there are inline attributes */
  width: 100% !important;
  height: auto !important;
}

.gallery-wrapper {
	
}

/* News
=====================================================================*/
#news {
	background: url('../img/theheron_bg1.jpg');
	background-position: center top;
	color: #2f2b28;
}
#news h1 {
	font-family: "ff-tisa-web-pro",serif;
	font-style: normal;
	font-weight: 400;
	font-size: 96px;
	line-height: 0;
}
#news p, #news li {
	font-family: "ff-tisa-web-pro",serif;
	font-style: normal;
	font-weight: 400;
	font-size: 16px;
}
#news li {
	list-style: none;
}
.news-wrapper {
	width: 60%;
	background: #f8efe6;
	background: rgb(248, 239, 230);
	background: rgba(248, 239, 230, 0.75);
	margin: 200px auto 100px auto;
	padding: 20px;
	position: relative;
	border: 20px solid #2f2b28;
}

#slider div div {
	width: 660px;
	
}
.prevBtn, .nextBtn {display:block;width:36px;height:77px;position:absolute;left:-76px;top:42%;z-index:1000;background:url(../img/btn_prev.png) no-repeat 0 0;cursor:pointer;}	
.nextBtn{left:741px;}														
.nextBtn {background:url(../img/btn_next.png) no-repeat 0 0;}

.hide-text {
text-indent: 100%;
white-space: nowrap;
overflow: hidden;
}

.news-date {
	font-family: "proxima-nova-condensed",sans-serif!important;
	font-style: normal;
	font-weight: 400;
	font-size:22px!important;
}

/* Menu
=====================================================================*/

#menu {
	position: relative;
	background: #2f2b28;
	color: #d4c6ab;
	padding-bottom: 100px;
}

.menu-title-wrapper {
	position:relative;
}

#menu h1 {
	font-family: "ff-tisa-web-pro",serif;
	font-style: normal;
	font-weight: 700;
	font-size: 124px;
	position:absolute;
	top:260px;
	left:-160px;
	transform:rotate(270deg);
	-ms-transform:rotate(270deg); /* IE 9 */
	-webkit-transform:rotate(270deg); /* Safari and Chrome */
}

#menu ul {
	list-style:none;
	padding:0;
}

#menu h2 {
	font-family: "proxima-nova-condensed",sans-serif;
	font-style: normal;
	font-weight: 300!important;
	font-size:60px;
	text-transform:uppercase;
	letter-spacing: 8px;
}

#menu h3 {
	font-family: "ff-tisa-web-pro",serif;
	font-style: normal;
	font-weight: 700;
	font-size: 26px;
	display:block;
	margin-bottom:0;
}

#menu li {
	font-family: "ff-tisa-web-pro",serif;
	font-style: normal;
	font-weight: 400;
	line-height: 2em;
	font-size: 18px;
	display:block;
}

#menu p {
	font-family: "proxima-nova-condensed",sans-serif;
	font-style: normal;
	font-weight: 400;
	line-height: 2em;
	font-size: 18px;
}

#menu small {
	display: block;
	font-style: italic;
}

#menu h3 + li {
	margin-top:20px;
}

#menu small + li {
	margin-top:40px;
}

#menu img {
	/*border: 10px solid #FFF;*/
}

.note {
	font-family: "proxima-nova-condensed",sans-serif!important;
	font-style: normal;
	font-weight: 300!important;
	font-size:14px;
	text-transform:uppercase;
	display:block;
	margin-top:0!important;
	
}

.price {
	font-weight: 700;
	padding-left:10px;
}

.menu-brunch, .menu-dinner, .menu-drinks {
	text-align: center;
}

.menu-brunch, .menu-dinner {
	border-bottom: 1px solid #d4c6ab;
}

.seperator {
	padding-left: 10px;
}

/* Hours
=====================================================================*/
#hours {
	background: #d5c6ab;
	color: #2f2b28;
	position: relative;
	padding-bottom: 100px;
}

#hours ul {
	padding: 0;
	margin-top:0;
}

#hours li {
	list-style: none;
	float:left;
}

#hours li:nth-child(1) {
	margin-left:0;
}

#hours li:nth-child(7) {
	margin-right:0;
}

#hours h1 {
	font-family: "ff-tisa-web-pro",serif;
	font-style: normal;
	font-weight: 400;
	font-size: 90px;
	margin-bottom: 0;
}

.hours-column {
	width: 124px;
	margin-left: 18px;
	margin-right: 18px;
	text-align: center;
}

#hours h2 {
	font-family: "ff-tisa-web-pro",serif;
	font-style: normal;
	font-weight: 400;
	font-size: 62px;
	color: #d5c6ab;
	padding-top: 15px;

}

.hours-day {
	background: #2f2b28;
	border-radius: 50%;
	width: 124px;
	height: 124px;
}

.the-hours {
	font-family: "ff-tisa-web-pro",serif;
	font-style: normal;
	font-weight: 400;
	font-size: 12px;
	margin:30px 0;
	display:block;
}

/* Our Story
=====================================================================*/
#our-story {
	background: #efeede;
	position: relative;
	padding-bottom: 100px;
}

#our-story h1 {
	font-family: "ff-tisa-web-pro",serif;
	font-style: normal;
	font-weight: 400;
	font-size: 90px;
	color: #575054;
}

#our-story p, #our-story blockquote {
	font-family: "ff-tisa-web-pro",serif;
	font-style: normal;
	font-weight: 400;
	font-size: 22px;
	color: #36484f;
}

#our-story blockquote {
	font-size:32px;
	font-style: italic;
	text-indent: -0.3em;
	margin-left:-60px;
}

#our-story img {
	margin-top:50%;
}

/* Catering
=====================================================================*/
.catering-narrow {
	display: none;
}

#catering {
	background: #ecddb9;
	position: relative;
	padding-bottom: 100px;
}

#catering h1 {
	font-family: "ff-tisa-web-pro",serif;
	font-style: normal;
	font-weight: 400;
	font-size: 72px;
	color: #7b3b31;
}

#catering p {
	font-family: "ff-tisa-web-pro",serif;
	font-style: normal;
	font-weight: 400;
	font-size: 22px;
	color: #36484f;
}

#catering img {
	width: 100%;
	margin-top: 100px;
	/*border: 4px solid #FFF;*/
}

/* Suppliers
=====================================================================*/
#suppliers {
	background: #b7c09e;
	color: #3a2d28;
	position: relative;
	padding-bottom: 100px;
}

#suppliers h1 {
	font-family: "ff-tisa-web-pro",serif;
	font-style: normal;
	font-weight: 400;
	font-size: 72px;
}

#suppliers p {
	font-family: "ff-tisa-web-pro",serif;
	font-style: normal;
	font-weight: 400;
	font-size: 22px;
}

.supplier-links {
	text-align: center;
}

#suppliers a {
	font-family: "ff-tisa-web-pro",serif;
	font-style: normal;
	font-weight: 400;
	font-size: 22px;
	color:#733128;
	padding:0 10px;
}
#suppliers img {
	margin-top: 210px;
	/*border: 10px solid #FFF;*/
}

/* Contact Us
=====================================================================*/
#contact-us {
	background: #2f2b28;
	color: #ebdfd6;
	position: relative;
	padding-bottom: 100px;
}

#contact-us h1 {
	font-family: "proxima-nova-condensed",sans-serif;
	font-style: normal;
	font-weight: 300;
	text-transform:uppercase;
	font-size:36px;
	margin-top: 80px;
}

#contact-us p {
	font-family: "ff-tisa-web-pro",serif;
	font-style: normal;
	font-weight: 400;
	font-size: 22px;
}

#contact-us a {
	font-family: "ff-tisa-web-pro",serif;
	font-style: normal;
	font-weight: 400;
	font-size: 22px;
	color: #ebdfd6;;
}

.iframe-rwd  {
	position: relative;
	padding-bottom: 65.25%;
	padding-top: 30px;
	height: 0;
	overflow: hidden;
}
.iframe-rwd iframe {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
}

.map {
	margin-top:80px;
}


/* MOBILE NAVIGATION
  -----------------------------------------------*/
#mobile-navigation {
  width: 100%;
  padding: 0;
  text-align: center;
}

#navmob {
	margin-bottom:0;
}

#navmob li {
	display:block;
	width:100%;
	padding: 10px 0;
}

#mobile-navigation {
  display: none;
  cursor: pointer;
  border-radius: 0px;
  background-color: #f8efe6;
  -moz-user-select: -moz-none;
  -webkit-user-select: none;
  -ms-user-select: none;
  user-select: none;
	position: fixed;
	top: 0;
	width: 100%;
	z-index: 9999;
}
#mobile-navigation #mobile-navigation-label {
  color: #2f2b28;
  background: #f8efe6;
  border-radius: 0px;
  cursor: pointer;
}

#mobile-navigation ul {
  display: none;
  list-style: none;
  padding: 0;
}
#mobile-navigation ul a {
  color: #2f2b28;
  text-decoration: none;
}
#mobile-navigation ul {
  display: none;
}

#mobile-navigation-label,
#mobile-navigation a {
	font-family: $font-sans;
	font-style: normal;
	font-weight: 400;
	text-transform:uppercase;
	font-size: 18px;
	padding: 0;
	text-align: center;
	display:block;
}

#mobile-navigation li {
	margin-bottom:0;
	border-bottom:1px solid #CCC;
}

#mobile-navigation-label {
  color: #fff;
  background: #000;
  font-weight:600;
  
}

#menu-icon {
	
	
}

.mobile-logo {
	padding: 0;
}
